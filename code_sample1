//Librerias
#include <stdio.h>
#include <stdint.h>


//Variables enteras con signo
//int8_t   0 - 127     -1 - -128
//int16_t  0 - 32767   -1 - -32768
//int32_t  0 - 2147483647  -1 - -2147483648
//int64_t  0 - 9223372036854775807   -1 - -9223372036854775808 

//Variables enteras sin signo
//uint8_t  0 - 255
//uint16_t 0 - 65535
//uint32_t 0 - 4294967295
//uint64_t 0 - 18446744073709551615

//Variables
uint8_t valor1=201;
uint8_t valor2=55;
uint16_t resultado=0;

//Funcion main
int main(){
    //SUmatoria
    resultado = valor1 + valor2;
    //Imprime en el terminal 
    printf("%u",resultado);
    //FInaliz√≥ correctamente
    return 0;
}
